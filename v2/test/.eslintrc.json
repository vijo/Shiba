{
  "extends": "eslint:recommended",
  "rules": {
    "indent": [
      2,
      4
    ],
    "quotes": 0,
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      2,
      "always"
    ],
    "no-console": 0
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true,
    "mocha": true
  }
}
